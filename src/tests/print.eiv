arr = {
    constructor = (arg){
        this.array = arg;
    };

    _display_ = (){
        first = true;

        for i = 0; i < this.array.length; i += 1{
            if !first{
                print(',');
            } 

            print(this.array[i]); 
            first = false;
        }
    };
};

inst = arr([1,2,3]);

println(inst);
